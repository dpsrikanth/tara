                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group has-placeholder">
                                        <label for="">User Type <span class="required">*</span></label>
                                        <select class="form-control" name='userType' value="<%=user.userType%>"
                                            required>
                                            <option value="0">Select from list</option>
                                            <option value="normal" <%=(user.userType==='normal' ) ? "selected" : "" %>
                                                >Regular User</option>
                                            <option value="memsa" <%=(user.userType==='memsa' ) ? "selected" : "" %>
                                                >MEMSA User</option>
                                            <option value="admin" <%=(user.userType==='admin' ) ? "selected" : "" %>
                                                >Admin User</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-6" id="divmemsa">
                                    <label for="">MEMSA ID<span class="required">*</span></label>
                                    <div class="form-group has-placeholder">

                                        <input aria-required="true" size="30" required name="mesmaId" id="mesmaId"
                                            class="form-control" placeholder="Enter your MEMSA ID"
                                            value="<%=user.mesmaId%>">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group has-placeholder">
                                        <label for="">Username <span class="required">*</span></label>
                                        <input type="text" aria-required="true" size="30" required name="username" id=""
                                            class="form-control" value="<%=user.username%>"
                                            placeholder="Enter your username">
                                    </div>
                                </div>
                                <%if (currentUser && currentUser.isAdmin) {%>
                                    <div class="col-lg-6">
                                        <div class="form-group has-placeholder">
                                            <label for="">Status <span class="required">*</span></label>
                                            <select class="form-control" name='status' value="<%=user.status%>"
